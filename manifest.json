{
    "manifest_version": 2,
    "name": "puglife",
    "version": "1.0.0",
    "description": "Pugs are life",
    "content_scripts": [ {
      "matches": [ "<all_urls>" ],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    } ],
    "web_accessible_resources": ["*.jpg", "*.jpeg","*.JPG", ".png", "."]
  }